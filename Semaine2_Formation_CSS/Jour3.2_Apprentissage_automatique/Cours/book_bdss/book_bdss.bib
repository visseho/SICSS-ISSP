@article{leavitt_story_2011,
	title = {Story Spoilers Don't Spoil Stories},
	url = {http://dx.doi.org/10.1177/0956797611417007},
	abstract = {Some people will go to great lengths to avoid story spoilers for a popular book or {TV} show, but this new study shows that spoilers don't actually diminish how much people enjoy a story. Volunteers read three short stories–one in which there was a paragraph before the story that gave away the ending, another in which the spoiler was incorporated into the first paragraph of the story, and an unspoiled story. Overall, the volunteers enjoyed the spoiled stories more than the unspoiled stories; therefore, spoilers may actually increase suspense instead of decreasing it.},
	journaltitle = {Psychological Science},
	author = {Leavitt, Jonathan D. and Christenfeld, Nicholas J. S.},
